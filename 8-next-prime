public Integer nextPrime(Integer num) {
    
    Integer n = num + 1;
    while (!IsPrime(n)) {
        n++
    }
    return n;

    public Boolean IsPrime(Integer n){
        Integer counter=0;

        for(Integer i=1;i<=n;i++){

            if(Math.mod(n,i)==0){
                counter++;
            }
        }

        if(counter==2){
            return true;
        }
        return false;
    }
}