public Boolean isLeapYear(Integer year) {
    if ((Math.mod((year / 4), 2) != 0)) {
        return true;
    } else {
        if (Math.mod(year / 100), 2) != 0) {
            return true;
        } else if (Math.mod(year / 400), 2) == 0) {
            return true;
        }
        else
            return false;
    }
}